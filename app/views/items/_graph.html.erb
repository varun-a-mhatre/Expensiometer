    <script type="text/javascript" src="https://www.google.com/jsapi"></script>

    <script type="text/javascript">

      google.load("visualization", "1", {packages:["corechart"]});
      google.setOnLoadCallback(drawChart);

      function drawChart() {
        var data = new google.visualization.DataTable();

        data.addColumn('string', 'Day');
        data.addColumn('number', '');
        data.addRows([
	<% @expenses.each do |expense| %>
		  ['<%= expense.monthweek %>',<%= expense.eaten %>],
	 <% end %>
      ]);

        var chart = new google.visualization.AreaChart(document.getElementById('calories_div'));
        chart.draw(data, {width: 330, height: 240, title: 'Calories Eaten', pointSize: '5', legend: 'none',
			  vAxis: {title: 'Calories' },
                          hAxis: {title: 'Week/Month'}
                         });
      }
    </script>

        <script type="text/javascript">

      google.load("visualization", "1", {packages:["corechart"]});
      google.setOnLoadCallback(drawChart);

      function drawChart() {
        var data = new google.visualization.DataTable();

        data.addColumn('string', 'Day');
        data.addColumn('number', '');
        data.addRows([
	<% @expenses.each do |expense| %>
		  ['<%= expense.monthweek %>',<%= expense.price %>],
	 <% end %>
      ]);

        var chart = new google.visualization.AreaChart(document.getElementById('expense_div'));
        chart.draw(data, {width: 330, height: 240, title: 'Expenses', colors: ['#FF0000'], pointSize: '5', legend: 'none',
			  vAxis: {title: '$' },
                          hAxis: {title: 'Week/Month'}
                         });
      }
    </script>

<div id="calories_div"></div>
<div id="expense_div"></div>
